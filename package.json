{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxi typecheck",
    "lint": "npm run lint:e && npm run lint:s",
    "lint:e": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:s": "stylelint '**/*.{css,scss,vue}' --ignore-path .gitignore",
    "format": "npm run format:e && npm run format:s",
    "format:e": "npm run lint:e -- --fix",
    "format:s": "npm run lint:s -- --fix",
    "test:ut": "vitest run"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/image": "^1.2.0",
    "@nuxt/test-utils": "^3.10.0",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitest/coverage-v8": "^1.2.2",
    "@vue/test-utils": "^2.4.4",
    "@vueuse/core": "^10.7.2",
    "@vueuse/nuxt": "^10.7.2",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^2.1.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "happy-dom": "^13.3.5",
    "nuxt": "^3.10.3",
    "playwright-core": "^1.41.1",
    "postcss": "^8.4.34",
    "postcss-html": "^1.6.0",
    "postcss-scss": "^4.0.9",
    "sass": "^1.69.7",
    "sass-loader": "^14.1.1",
    "stylelint": "^16.2.0",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2",
    "vue": "^3.4.14",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "camelcase-keys": "^9.1.3",
    "dayjs": "^1.11.10",
    "ress": "^5.0.2",
    "snakecase-keys": "^6.0.0",
    "vite-svg-loader": "^5.1.0"
  },
  "engines": {
    "node": "^20.11.1",
    "npm": "^10.2.4"
  },
  "volta": {
    "node": "20.11.1",
    "npm": "10.2.4"
  }
}
